# API Configuration
# Base URL for the backend API
REACT_APP_API_URL=https://api.example.com
# API request timeout in milliseconds
REACT_APP_API_TIMEOUT=30000

# Feature Flags
# Enable/disable 2D lottery feature
REACT_APP_ENABLE_2D=true
# Enable/disable 3D lottery feature
REACT_APP_ENABLE_3D=true

# App Configuration
# Minimum bet amount in base currency
REACT_APP_MIN_BET_AMOUNT=100
# Minimum deposit amount in base currency
REACT_APP_MIN_DEPOSIT_AMOUNT=1000
# Maximum withdrawal amount in base currency
REACT_APP_MAX_WITHDRAW_AMOUNT=1000000

# Time Configuration (in minutes)
# Time before 2D lottery closes for betting
REACT_APP_2D_CLOSE_BEFORE=30
# Time before 3D lottery closes for betting
REACT_APP_3D_CLOSE_BEFORE=60

# Security Configuration
# JWT token expiry time in seconds
REACT_APP_JWT_EXPIRY=86400
# Enable/disable rate limiting
REACT_APP_ENABLE_RATE_LIMITING=true 